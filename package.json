    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@types/bcrypt": "^6.0.0",
    "@types/node": "^20",
    "@types/pg": "^8.15.6",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@vitejs/plugin-react": "^5.1.1",
    "@vitest/ui": "^4.0.15",
    "eslint": "^9",
    "eslint-config-next": "^16.0.8",
    "eslint-config-prettier": "^10.1.8",
    "jsdom": "^27.2.0",
    "prettier": "^3.6.2",
    "prisma": "^7.0.1",
    "tsx": "^4.21.0",
    "typescript": "^5",
    "vitest": "^4.0.15"
  }
